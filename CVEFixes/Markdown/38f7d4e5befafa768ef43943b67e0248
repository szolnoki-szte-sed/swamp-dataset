     STRUCT_NAME => 'ACDSee Dimensions',
     NAMESPACE   => {'acdsee-stDim' => 'http://ns.acdsee.com/sType/Dimensions#'},
-    'w'           => { Writable => 'real' },
-    'h'           => { Writable => 'real' },
-    'unit'        => { },
+    'w'         => { Writable => 'real' },
+    'h'         => { Writable => 'real' },
+    'unit'      => { },
 );
 
